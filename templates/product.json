        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>InstantID Image Generator</title>\n</head>\n<body>\n    <h1>InstantID Image Generator</h1>\n    \n    <form id=\"upload-form\">\n        <label for=\"input_imgs\">Input Images:</label><br>\n        <input type=\"file\" id=\"input_imgs\" name=\"input_imgs\" multiple required><br><br>\n\n        <label for=\"style_img\">Style Image:</label><br>\n        <input type=\"file\" id=\"style_img\" name=\"style_img\" required><br><br>\n\n        <label for=\"keypoints_img\">Keypoints Image:</label><br>\n        <input type=\"file\" id=\"keypoints_img\" name=\"keypoints_img\" required><br><br>\n\n        <button type=\"submit\">Generate</button>\n    </form>\n\n    <div id=\"result\">\n        <h2>Generated Image:</h2>\n        <img id=\"output-image\" style=\"max-width: 500px; display: none;\" />\n    </div>\n\n    <script>\n        document.getElementById(\"upload-form\").addEventListener(\"submit\", async function(event) {\n            event.preventDefault(); // Ngăn form reload trang\n\n            const formData = new FormData();\n            const inputImgs = document.getElementById(\"input_imgs\").files;\n            const styleImg = document.getElementById(\"style_img\").files[0];\n            const keypointsImg = document.getElementById(\"keypoints_img\").files[0];\n\n            // Thêm các file vào formData\n            for (let i = 0; i < inputImgs.length; i++) {\n                formData.append(\"input_imgs\", inputImgs[i]);\n            }\n            formData.append(\"style_img\", styleImg);\n            formData.append(\"keypoints_img\", keypointsImg);\n\n            try {\n                const response = await fetch(\"https://category-fully-finding-suspended.trycloudflare.com/generate\", {\n                    method: \"POST\",\n                    body: formData\n                });\n\n                if (!response.ok) {\n                    throw new Error(`Error: ${response.statusText}`);\n                }\n\n                const blob = await response.blob(); // Lấy ảnh trả về dạng blob\n                const imageUrl = URL.createObjectURL(blob);\n\n                const outputImage = document.getElementById(\"output-image\");\n                outputImage.src = imageUrl;\n                outputImage.style.display = \"block\"; // Hiển thị ảnh sau khi nhận được\n            } catch (error) {\n                console.error(\"Error generating image:\", error);\n                alert(\"Failed to generate image. Please try again.\");\n            }\n        });\n    </script>\n</body>\n</html>",
